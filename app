= request.form['message']\n","\n","        # Connect to DB and update the contact with contact_id\n","        conn = sqlite3.connect(db_path)\n","        cursor = conn.cursor()\n","        update_query = '''\n","        UPDATE Contact\n","        SET name = ?, email = ?, phone = ?, subject = ?, message = ?\n","        WHERE id = ?\n","        '''\n","        cursor.execute(update_query, (new_name, new_email, new_phone, new_subject, new_message, contact_id))\n","        conn.commit()\n","        conn.close()\n","        flash(\"Contact updated successfully\", \"success\")\n","        return redirect(url_for('bootstrap'))  # Replace 'admin_page' with the name of your admin route\n","    else:\n","        # Display the current data in an HTML form\n","        conn = sqlite3.connect(db_path)\n","        cursor = conn.cursor()\n","        select_query = 'SELECT * FROM Contact WHERE id = ?'\n","        cursor.execute(select_query, (contact_id,))\n","        contact = cursor.fetchone()\n","        conn.close()\n","        return render_template('bootstrap', contact=contact)  # Ensure you have an update_contact.html template\n","\n","\n","@app.route(\"/delete/<int:contact_id>\")\n","def delete_contact(contact_id):\n","    conn = sqlite3.connect(db_path)  # Use db_path defined previously\n","    cursor = conn.cursor()\n","    delete_query = 'DELETE FROM Contact WHERE id = ?'\n","    cursor.execute(delete_query, (contact_id,))\n","    conn.commit()\n","    conn.close()\n","    flash(\"Contact deleted successfully\", \"success\")\n","    return redirect(url_for('bootstrap'))  # Redirect to the appropriate page\n","\n","\n","@app.route(\"/login\",methods= ['GET', 'POST'])\n","def login():\n","  if request.method==\"POST\":\n","    username=request.form.get(\"email\")\n","    password=request.form.get(\"password\")\n","    user = validate_user(username, password)\n","    if user:\n","       return redirect(url_for('bootstrap'))\n","    else:\n","      message=\"Wrong username or password.Please try again!\"\n","      return render_template(\"login.html\",message=message)\n","  else:\n","       return render_template(\"login.html\")\n","\n","@app.route(\"/logout\")\n","def logout():\n","    # Clear the session\n","    session.clear()\n","    # Redirect to the home page\n","    return redirect(url_for('login'))\n","\n","\n","if __name__ == \"__main__\":\n","    app.run()\n","\n","# Optional: Retrieve data to verify insertion\n","select_query = 'SELECT * FROM Contact'\n","cursor.execute(select_query)\n","rows = cursor.fetchall()\n","for row in rows:\n","    print(row)"]},{"cell_type":"code","execution_count":39,"metadata":{"id":"wvs0uvTWhGug","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1705202983962,"user_tz":-480,"elapsed":16,"user":{"displayName":"Albert Chong","userId":"16445831990029109488"}},"outputId":"023ce5c9-6de9-4593-c37f-3f15aaca560c"},"outputs":[{"output_type":"stream","name":"stdout","text":["(3, 'ABC', 'abc@example.com', 1234567890, 'abc', 'abc')\n","(5, '123', 'aaaa@gmail.com', 'aaaaaa', 'aa', 'aaaaaaa')\n","(7, 'John Doe', 'john@example.com', 8888888888, 'Sample Subject', 'Sample message')\n","(28, 'test', 'john@example.com', 1234567890, 'Sample Subject', 'Sample message')\n","(31, 'llll', 'lll@gmail.com', 8888888888, 'fff', 'fff')\n","(32, 'ppp', 'john@example.com', 1234567890, 'Sample Subject', 'Sample message')\n","(33, 'John Doe', 'john@example.com', 1234567890, 'Sample Subject', 'Sample message')\n","(34, 'John Doe', 'john@example.com', 1234567890, 'Sample Subject', 'Sample message')\n","(35, 'John Doe', 'john@example.com', 1234567890, 'Sample Subject', 'Sample message')\n","(36, 'John Doe', 'john@example.com', 1234567890, 'Sample Subject', 'Sample message')\n","(37, 'John Doe', 'john@example.com', 1234567890, 'Sample Subject', 'Sample message')\n","(38, 'John Doe', 'john@example.com', 1234567890, 'Sample Subject', 'Sample message')\n","(39, 'John Doe', 'john@example.com', 123