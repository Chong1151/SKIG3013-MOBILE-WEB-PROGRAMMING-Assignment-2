 ContactForm()\n","    if request.method == 'POST':\n","        if form.validate_on_submit():\n","            try:\n","                # Connect to the SQLite database\n","                conn = sqlite3.connect(db_path)\n","                cursor = conn.cursor()\n","\n","                # Insert new contact\n","                insert_query = '''\n","                INSERT INTO Contact (name, email, phone, subject, message)\n","                VALUES (?, ?, ?, ?, ?)\n","                '''\n","                cursor.execute(insert_query, (form.name.data, form.email.data, form.phone.data, form.subject.data, form.message.data))\n","                conn.commit()\n","                conn.close()\n","\n","                session['message'] = 'Your message has been sent. Thank you!'\n","                return redirect(url_for('home'))\n","            except Exception as e:\n","                session['message'] = 'An error occurred. Please try again.'\n","                app.logger.error('Error on form submission: %s', e)\n","                conn.rollback()\n","                conn.close()\n","        else:\n","            # Collect form errors\n","            form_errors = {field: error for field, error in form.errors.items()}\n","            return render_template('index.html', form=form, form_errors=form_errors)\n","    return render_template('index.html', form=form)\n","\n","#Contact form route\n","@app.route(\"/contactadmin\", methods=['GET', 'POST'])\n","def contactadmin():\n","    form = ContactForm()\n","    if request.method == 'POST':\n","        if form.validate_on_submit():\n","            try:\n","                # Connect to the SQLite database\n","                conn = sqlite3.connect(db_path)\n","                cursor = conn.cursor()\n","\n","                # Insert new contact\n","                insert_query = '''\n","                INSERT INTO Contact (name, email, phone, subject, message)\n","                VALUES (?, ?, ?, ?, ?)\n","              